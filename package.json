{
  "name": "debrid-2.0",
  "version": "1.0.1",
  "description": "An easy way to search and download for torrents via real debrid.",
  "main": "app.js",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "sw-precache": "^5.0.0"
  },
  "scripts": {
    "build:css": "postcss public/assets/styles.css -o public/assets/styles.autoprefixed.css",
    "watch:css": "postcss public/assets/styles.css -o public/assets/styles.autoprefixed.css --watch",
    "build:sw": "sw-precache --config config/sw-precache-config.js --verbose",
    "dev": "concurrently \"postcss public/assets/styles.css -o public/assets/styles.autoprefixed.css --watch\" \"nodemon app.js\"",
    "start": "node app.js"
  },
  "author": "BakaBoi341",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "postcss-cli": "^11.0.0",
    "sw-precache": "^5.0.0"
  }
}
